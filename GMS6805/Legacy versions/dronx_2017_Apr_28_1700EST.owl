    <!-- http://purl.obolibrary.org/obo/dronx/DRON_00005004 -->

    <owl:Class rdf:about="&obo;dronx/DRON_00005004">
        <rdfs:label xml:lang="en">anti-hypertensive function-activating alpha-2-adrenergic receptor disposition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;DRON_00005008"/>
        <obo:IAO_0000115 xml:lang="en">A function-altering anti-hypertensive disposition of a molecule to bind to an instance of an alpha-2-adrenergice receptor in a manner that activates the receptor.</obo:IAO_0000115>
        <obo:IAO_0000117>Matthew Diller</obo:IAO_0000117>
        <obo:IAO_0000117>Caitrin McDonough Rowe</obo:IAO_0000117>
        <obo:IAO_0000119 xml:lang="en">Goodman &amp; Gilman’s The Pharmacological Basis of Therapeutics, 12th Edition, Chapter 12, Alpha1-Selective Adrenergic Receptor Agonists</obo:IAO_0000119>
        <obo:IAO_0000118 xml:lang="en">alpha-2-adrenergeic agonists</obo:IAO_0000118>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/dronx/DRON_00005005 -->

    <owl:Class rdf:about="&obo;dronx/DRON_00005005">
        <rdfs:label xml:lang="en">anti-hypertensive function-activating adenosine triphosphate-modulated potassium (ATP-modulated K+) channel disposition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;DRON_00005008"/>
        <obo:IAO_0000115 xml:lang="en">A function-altering anti-hypertensive disposition of a molecule to bind to an instance of an adenosine triphosphate-modulated potassium (ATP-modulated K+) channel in a manner that opens the channel and permits potassium efflux.</obo:IAO_0000115>
        <obo:IAO_0000117>Caitrin McDonough Rowe</obo:IAO_0000117>
        <obo:IAO_0000119 xml:lang="en">Goodman &amp; Gilman’s The Pharmacological Basis of Therapeutics, 12th Edition, Chapter 27, Vasodilators</obo:IAO_0000119>
        <obo:IAO_0000117>Matthew Diller</obo:IAO_0000117>
        <obo:IAO_0000118 xml:lang="en">adenosine triphosphate-modulate potassium channel activator</obo:IAO_0000118>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/dronx/DRON_00005006 -->

    <owl:Class rdf:about="&obo;dronx/DRON_00005006">
        <rdfs:label xml:lang="en">anti-hypertensive function-inhibiting vesicular catecholamine transporter activity disposition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;DRON_00005008"/>
        <obo:IAO_0000115 xml:lang="en">A disposition of a molecule to bind to an instance of a vesicular catecholamine transporter in a manner that inhibits the transporter activity.</obo:IAO_0000115>
        <obo:IAO_0000117>Matthew Diller</obo:IAO_0000117>
        <obo:IAO_0000117>Caitrin McDonough Rowe</obo:IAO_0000117>
        <obo:IAO_0000119 xml:lang="en">Goodman &amp; Gilman’s The Pharmacological Basis of Therapeutics, 12th Edition, Chapter 27, Combined Alpha-1 and Beta Adrenergic Receptor Antagonists</obo:IAO_0000119>
        <obo:IAO_0000118 xml:lang="en">Vesicular catecholamine transporter inhibitor</obo:IAO_0000118>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/dronx/DRON_00005007 -->

    <owl:Class rdf:about="&obo;dronx/DRON_00005007">
        <rdfs:label xml:lang="en">anti-hypertensive function-activating imidazoline receptor subtype 1 disposition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;DRON_00005008"/>
        <obo:IAO_0000115 xml:lang="en">A function-altering anti-hypertensive disposition of a molecule to bind to an instance of an imidazoline receptor subtype 1 (I1) in a manner that activates the receptor.</obo:IAO_0000115>
        <obo:IAO_0000117>Caitrin McDonough Rowe</obo:IAO_0000117>
        <obo:IAO_0000118 xml:lang="en">Imidazoline receptor subtype 1 agonist</obo:IAO_0000118>
        <obo:IAO_0000117>Matthew Diller</obo:IAO_0000117>
        <obo:IAO_0000119 xml:lang="en">PubMed ID: 9050981 (Ernsberger P et al. The I1-imidazoline receptor: from binding to therapeutic target in cardiovascular disease. J Hypertens Suppl. 1997 Jan;15(1): S9-S23.)</obo:IAO_0000119>
    </owl:Class>
    


