    <!-- http://purl.obolibrary.org/obo/DRON_00005000 -->

    <owl:Class rdf:about="&obo;DRON_00005000">
        <rdfs:label xml:lang="en">anti-hypertensive function-inhibiting alpha-1-adrenergic receptor binding disposition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;DRON_00005008"/>
        <obo:IAO_0000117>Caitrin McDonough Rowe</obo:IAO_0000117>
        <obo:IAO_0000115 xml:lang="en">A function-altering anti-hypertensive disposition of a molecule to bind to an instance of an alpha-1-adrenergic receptor in a manner that inhibits the realization of a function of the receptor instance.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Goodman and Gilman&apos;s The Pharmacological Basis of Therapeutics, 12th Edition. Section II. Chapter 12 - Adrenergic Agonists and Antagonists.</obo:IAO_0000119>
        <obo:IAO_0000117>Matthew Diller</obo:IAO_0000117>
        <obo:IAO_0000118 xml:lang="en">alpha-1-adrenergic receptor antagonist</obo:IAO_0000118>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DRON_00005001 -->

    <owl:Class rdf:about="&obo;DRON_00005001">
        <rdfs:label xml:lang="en">anti-hypertensive function-inhibiting epithelial sodium channel binding anti-hypertensive disposition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;DRON_00005008"/>
        <obo:IAO_0000115 xml:lang="en">A function-altering anti-hypertensive disposition of a molecule to bind to an instance of an epithelial sodium channel in a manner that inhibits the realization of a function of the sodium channel instance.</obo:IAO_0000115>
        <obo:IAO_0000117>Matthew Diller</obo:IAO_0000117>
        <obo:IAO_0000117>Caitrin McDonough Rowe</obo:IAO_0000117>
        <obo:IAO_0000119 xml:lang="en">Goodman and Gilman&apos;s The Pharmacological Basis of Therapeutics, 12th Edition. Section III. Chapter 25 - Antagonists of Mineralocorticoid Receptors (Aldosterone Antagonists, Potassium-Sparing Diuretics).</obo:IAO_0000119>
        <obo:IAO_0000118 xml:lang="en">function-inhibiting amiloride-sensitive sodium channel binding disposition</obo:IAO_0000118>
        <obo:IAO_0000118 xml:lang="en">potassium-sparing disposition</obo:IAO_0000118>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DRON_00005002 -->

    <owl:Class rdf:about="&obo;DRON_00005002">
        <rdfs:label xml:lang="en">anti-hypertensive function-inhibiting mineralocorticoid receptor binding disposition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;DRON_00005008"/>
        <obo:IAO_0000117>Caitrin McDonough Rowe</obo:IAO_0000117>
        <obo:IAO_0000115 xml:lang="en">A function-altering anti-hypertensive disposition of a molecule to bind to an instance of a mineralocorticoid receptor in a manner that competitively inhibits the binding of an instance of aldosterone.</obo:IAO_0000115>
        <obo:IAO_0000117>Matthew Diller</obo:IAO_0000117>
        <obo:IAO_0000119 xml:lang="en">Goodman and Gilman&apos;s The Pharmacological Basis of Therapeutics, 12th Edition. Section III. Chapter 25 - Antagonists of Mineralocorticoid Receptors (Aldosterone Antagonists, Potassium-Sparing Diuretics).</obo:IAO_0000119>
        <obo:IAO_0000118 xml:lang="en">aldosterone antagonist</obo:IAO_0000118>
        <obo:IAO_0000118 xml:lang="en">function-competitive inhibition of mineralocorticoid receptor binding disposition</obo:IAO_0000118>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DRON_00005003 -->

    <owl:Class rdf:about="&obo;DRON_00005003">
        <rdfs:label xml:lang="en">anti-hypertensive function-inhibiting renin catalytic activity disposition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;DRON_00005008"/>
        <obo:IAO_0000117>Caitrin McDonough Rowe</obo:IAO_0000117>
        <obo:IAO_0000117>Matthew Diller</obo:IAO_0000117>
        <obo:IAO_0000115 xml:lang="en">A function-altering anti-hypertensive disposition of a molecule to directly and competitively inhibit the catalytic activity of an instance of renin in a manner that leads to diminished production of AngI (angiotensin I).</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Goodman and Gilman&apos;s The Pharmacological Basis of Therapeutics, 12th Edition. Section III. Chapter 27 - Direct Renin Inhibitors</obo:IAO_0000119>
        <obo:IAO_0000118 xml:lang="en">direct renin inhibitor</obo:IAO_0000118>
        <obo:IAO_0000118 xml:lang="en">function-direct and competitive inhibition of renin activity</obo:IAO_0000118>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DRON_00005008 -->

    <owl:Class rdf:about="&obo;DRON_00005008">
        <rdfs:label xml:lang="en">function-altering anti-hypertensive disposition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;BFO_0000016"/>
        <obo:IAO_0000117>Matthew Diller</obo:IAO_0000117>
        <obo:IAO_0000117>Caitrin McDonough Rowe</obo:IAO_0000117>
        <obo:IAO_0000115 xml:lang="en">A disposition of a molecule to bind to some instance of a protein in a manner that 1) changes the realization of some function or role inhering in that protein, and 2) contributes to a decrease in blood pressure.</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DRON_00005009 -->

    <owl:Class rdf:about="&obo;DRON_00005009">
        <rdfs:label xml:lang="en">anti-hypertensive non-activating competitive angiotensin receptor binding disposition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;DRON_00005008"/>
        <obo:IAO_0000117>Caitrin McDonough Rowe</obo:IAO_0000117>
        <obo:IAO_0000115 xml:lang="en">A function-altering anti-hypertensive disposition of a molecule to bind to an instance of an angiotensin subtype 1 (AT1) receptor in a manner that blocks the biological effects of angiotensin II (AngII).</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Goodman &amp; Gilman’s The Pharmacological Basis of Therapeutics, 12th Edition, Chapter 26, Section Non-Peptide Angiotensin II Receptor Antagonists</obo:IAO_0000119>
        <obo:IAO_0000117>Matthew Diller</obo:IAO_0000117>
        <obo:IAO_0000118 xml:lang="en">angiotensin II receptor antagonist</obo:IAO_0000118>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DRON_00005010 -->

    <owl:Class rdf:about="&obo;DRON_00005010">
        <rdfs:label xml:lang="en">anti-hypertensive non-activating competitive beta-adrenergic receptor binding disposition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;DRON_00005008"/>
        <obo:IAO_0000115 xml:lang="en">A function-altering anti-hypertensive disposition of a molecule to bind to an instance of a beta-adrenergic receptor in a manner that blocks sympathetic stimulation mediated by beta-adrenergic receptors in the heart and vascular smooth muscle.</obo:IAO_0000115>
        <obo:IAO_0000117>Caitrin McDonough Rowe</obo:IAO_0000117>
        <obo:IAO_0000119 xml:lang="en">Goodman &amp; Gilman’s The Pharmacological Basis of Therapeutics, 12th Edition, Chapter 27, Section Sympatholythic Agents</obo:IAO_0000119>
        <obo:IAO_0000117>Matthew Diller</obo:IAO_0000117>
        <obo:IAO_0000118 xml:lang="en">beta-adrenergic receptor blocker</obo:IAO_0000118>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DRON_00005011 -->

    <owl:Class rdf:about="&obo;DRON_00005011">
        <rdfs:label xml:lang="en">anti-hypertensive function-inhibiting sodium-chloride cotransporter binding disposition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;DRON_00005008"/>
        <obo:IAO_0000117>Caitrin McDonough Rowe</obo:IAO_0000117>
        <obo:IAO_0000115 xml:lang="en">A function-altering anti-hypertensive disposition of a molecule to bind in the distal convoluted tubule in a manner that inhibits sodium-chloride transport.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Goodman &amp; Gilman’s The Pharmacological Basis of Therapeutics, 12th Edition, Chapter 25, Section Inhibitors of Na+Cl- Symport (Thiazide and Thiazide-like Diuretics)</obo:IAO_0000119>
        <obo:IAO_0000117>Matthew Diller</obo:IAO_0000117>
        <obo:IAO_0000118 xml:lang="en">thiazide diuretic</obo:IAO_0000118>
        <obo:IAO_0000118 xml:lang="en">thiazide-like diuretic</obo:IAO_0000118>
    </owl:Class>
    


